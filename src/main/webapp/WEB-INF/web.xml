  <servlet>
  	<servlet-name>RechercheResult</servlet-name>
  	<servlet-class>com.appli.servlets.RechercheResult</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DetailSite</servlet-name>
  	<servlet-class>com.appli.servlets.DetailSite</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DetailSecteur</servlet-name>
  	<servlet-class>com.appli.servlets.DetailSecteur</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddSite</servlet-name>
  	<servlet-class>com.appli.servlets.AddSite</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Delete</servlet-name>
  	<servlet-class>com.appli.servlets.Delete</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddSecteur</servlet-name>
  	<servlet-class>com.appli.servlets.AddSecteur</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddVoie</servlet-name>
  	<servlet-class>com.appli.servlets.AddVoie</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddTopo</servlet-name>
  	<servlet-class>com.appli.servlets.AddTopo</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DetailVoie</servlet-name>
  	<servlet-class>com.appli.servlets.DetailVoie</servlet-class>
  </servlet>
