  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Delete</servlet-name>
  	<url-pattern>/delete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddSecteur</servlet-name>
  	<url-pattern>/addSecteur</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddVoie</servlet-name>
