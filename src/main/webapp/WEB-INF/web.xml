  	<url-pattern>/detailVoie</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DetailTopo</servlet-name>
  	<url-pattern>/detailTopo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddCom</servlet-name>
  	<url-pattern>/addCom</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AccesPublic</servlet-name>
  	<url-pattern>/acces_Public</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Modif</servlet-name>
  	<url-pattern>/modif</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Espace_user</servlet-name>
  	<url-pattern>/espace_user</url-pattern>
  </servlet-mapping>
  
  <filter>
    <filter-name>Restriction</filter-name>
    <filter-class>com.appli.filters.Restriction</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>Restriction</filter-name>
    <url-pattern>/addsite</url-pattern>
    <url-pattern>/addSecteur</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>Restriction</filter-name>
    <url-pattern>/AddVoie</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>Restriction</filter-name>
    <url-pattern>/addVoie</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>Restriction</filter-name>
    <url-pattern>/AddCom</url-pattern>
    <dispatcher>REQUEST</dispatcher>
  </filter-mapping>
  <filter-mapping>
    <filter-name>Restriction</filter-name>
    <url-pattern>/addCom</url-pattern>
    <dispatcher>REQUEST</dispatcher>
  </filter-mapping>
  <filter-mapping>
    <filter-name>Restriction</filter-name>
    <url-pattern>/AddTopo</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>Restriction</filter-name>
    <url-pattern>/addtopo</url-pattern>
  </filter-mapping>
  
  
  
  
  
<jsp-config>
    <jsp-property-group>
      <url-pattern>*.jsp</url-pattern>
      <include-prelude>/taglibs.jsp</include-prelude>
    </jsp-property-group>
  </jsp-config>
  
  
</web-app>

